generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  relationMode = "prisma"
}

model Expenses {
  id     String    @id @default(dbgenerated("(uuid())")) @db.VarChar(36)
  name   String @db.Text
  amount Float
}
